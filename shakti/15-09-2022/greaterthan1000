

    #include<iostream>
    using namespace std;
    int main(){
    int num;
    cin>>num;
    
    if (num>1000){
    cout<<num<<" is greater than 1000 ";
    }
    else if (num>500) {
       cout<<num<<" is greater than 500 "<<endl; 
      }
    else if (num>100){
       cout<<num<<" is greater than 100 "<<endl; 
      }
    else if (num>10){
       cout<<num<<"num is greater than 10 "<<endl; 
      }
   }
