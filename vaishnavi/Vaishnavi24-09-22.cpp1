// Print Pattern for numbers of rows and column
// 10 10 10 10 10 10 10 10 10 10
//  9 9 9 9 9 9 9 9 9
//  8 8 8 8 8 8 8 8
//  7 7 7 7 7 7 7
//  6 6 6 6 6 6
//  5 5 5 5 5
//  4 4 4 4
//  3 3 3
//  2 2
//  1

#include<iostream>
using namespace std;
int main()
{
    int i,j,n;
    cin>>n;

    for(i=n;i>=1;i--){
        for(j=1;j<=i;j++){
                cout<<" "<<i ;
            }
           cout<<endl;
    }
}
