class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        int a;
    sort(nums.begin(),nums.end());
         for(int i=1;i<nums.size();i++){
             if(nums[i]==nums[i-1]){      
                 a=nums[i];
                }
            }
      return a;
    }
};

Input
nums =[1,3,4,2,2]
Output
2
Expected
2

Input
nums =[3,1,3,4,2]
Output
3
Expected
3
