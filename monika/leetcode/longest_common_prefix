class Solution {
public:
    string longestCommonPrefix(vector<string>& strs) {
        string x="";
        if(strs.size()==0)
            return x;
        for( int i=0;i<strs[0].size();i++){
            char t=strs[0][i];
          for(int j=1;j<strs.size();j++){
              if(t!=strs[j][i])
                  return x;}
        x=x+t;}
        return x;
    }
};
